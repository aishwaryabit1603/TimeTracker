{
    "manifest_version": 3,
    "name": "Break Time Tracker",
    "version": "1.0",
    "permissions": [
        "notifications",
        "storage",
        "alarms"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "128": "icon128.png"
        }
    },
    "icons": {
        "128": "icon128.png"
    }
}